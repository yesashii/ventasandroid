<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ectableLayout1" >

                <TableRow
                    android:id="@+id/ectableRow01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/ecTextView01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Censo :  "
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />

					<Spinner
                    	android:id="@+id/ecspCenso"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:listitem="@android:layout/simple_spinner_item" />
                </TableRow>
                
                <TableRow
                    android:id="@+id/ectableRow02"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/ecTextView02"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Cantidad de PDV :  "
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />
                    
                    <TextView
                        android:id="@+id/ecTextView03"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textStyle="bold" />

                </TableRow>

                <TableRow
                    android:id="@+id/ectableRow03"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/ecTextView04"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Censados :  "
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />
                    
                    <TextView
                        android:id="@+id/ecTextView05"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textStyle="bold" />

                </TableRow>
                
                <TableRow
                    android:id="@+id/ectableRow05"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/ecTextView08"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Enviados :  "
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />
                    
                    <TextView
                        android:id="@+id/ecTextView09"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textStyle="bold" />

                </TableRow>
                
                <TableRow
                    android:id="@+id/ectableRow04"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/ecTextView06"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Cumplimiento :  "
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />
                    
                    <TextView
                        android:id="@+id/ecTextView07"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textStyle="bold" />

                </TableRow>
                
            </TableLayout>
            
    <Button
        android:id="@+id/ecbtncAtras"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AtrÃ¡s" />

    	
</LinearLayout>

